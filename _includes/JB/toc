<script type="text/javascript" src="assets/theme/jQuery v1.8.0.js"></script>

<script type="text/javascript">

$(document).ready(function(){
var ToC =
  "<nav role='navigation' class='table-of-contents'>" +
    "<h2>On this page:</h2>" +
    "<ul>";
var newLine, el, title, link;
$("article h3").each(function() {
  el = $(this);
  title = el.text();
  link = "#" + el.attr("id");
  newLine =
    "<li>" +
      "<a href='" + link + "'>" +
        title +
      "</a>" +
    "</li>";
  ToC += newLine;
});
ToC +=
   "</ul>" +
  "</nav>";
$(".all-questions").prepend(ToC);
});

</script>
